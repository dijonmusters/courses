{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel start:app start:functions",
    "build": "npm-run-all --parallel build:**",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start:app": "npm run develop",
    "start:functions": "netlify-lambda serve functions",
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "prebuild": "echo 'setup faunaDB' && npm run bootstrap",
    "build:app": "gatsby build",
    "build:functions": "netlify-lambda build functions",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@mdx-js/mdx": "^1.0.21",
    "@mdx-js/react": "^1.0.21",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.15",
    "apollo-server-lambda": "^2.9.3",
    "axios": "^0.19.0",
    "babel-plugin-styled-components": "^1.10.1",
    "dotenv": "^8.1.0",
    "gatsby": "^2.10.0",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-layout": "^1.1.0",
    "gatsby-plugin-netlify": "^2.1.0",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-remark-images": "^3.1.0",
    "gatsby-source-filesystem": "^2.1.0",
    "graphql": "^14.5.4",
    "mongoose": "^5.6.11",
    "netlify-identity-widget": "^1.5.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
